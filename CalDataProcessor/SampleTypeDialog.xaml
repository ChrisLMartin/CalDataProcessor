<Window x:Class="CalDataProcessor.SampleTypeDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CalDataProcessor"
        mc:Ignorable="d"
        Title="Sample Type" Height="200" Width="300">
    <Grid>
        <RadioButton Name="EFC" Content="EFC" HorizontalAlignment="Left" Margin="70,50,0,0" VerticalAlignment="Top"/>
        <RadioButton Name="OPC" Content="OPC" HorizontalAlignment="Left" Margin="70,70,0,0" VerticalAlignment="Top"/>
        <Button Content="Skip" Click="Skip_Click" HorizontalAlignment="Left" Margin="70,90,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="Select" Click="Select_Click" HorizontalAlignment="Left" Margin="160,90,0,0" VerticalAlignment="Top" Width="75"/>
        <ListBox Name="SampleName" HorizontalAlignment="Left" Height="20" Margin="70,26,0,0" VerticalAlignment="Top" Width="179"/>

    </Grid>
</Window>
